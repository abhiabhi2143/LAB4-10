package Day13;
import java.util.*;
public class Person {
	static Scanner sc=new Scanner(System.in);
	
	String fn;
	String ln;
	String gender;
	String ph;
	Person()
	{
		
	}
	Person(String fn,String ln,String gender){
		super();
		this.fn=fn;
		this.ln=ln;
		this.gender=gender;
	}
	String getFN()
	{
		return fn;
	}
	String getLN()
	{
		return ln;
	}
	String getGender()
	{
		return gender;
	}
	void dis()
	{
		System.out.println("firstName is "+getFN());
		System.out.println("lastName is "+getLN());
		System.out.println("gender is "+getGender());
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person x=new Person("abhiram","kondeti","male");
		x.dis();
	}

}
