package Day13;
import java.util.*;
public class Ex2 {
	static Scanner sc=new Scanner(System.in);
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n=sc.nextInt();
		if(n>0)
		{
			System.out.println("It is a positive number");
		}
		else
		{
			System.out.println("It is a negative number");
		}

	}

}
